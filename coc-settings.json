{
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "●",
  "diagnostic.infoSign": "●",
  "codeLens.enable": true,
  "suggest.echodocSupport": true,
  "metals.statusBarEnabled": true,
  "languageserver": {
    "clojure-lsp": {
      "command": "clojure-lsp",
      "rootPatterns": ["project.clj"],
      "filetypes": [ "clojure" ],
      "additionalSchemes": ["jar", "zipfile"]
    },
    "nix": {
      "command": "rnix-lsp",
      "filetypes": [ "nix" ]
    }
  }
}
