{
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "●",
  "diagnostic.infoSign": "●",
  "python.jediEnabled": false,
  "languageserver": {
    "metals": {
      "command": "metals-vim",
      "rootPatterns": ["build.sbt"],
      "filetypes": ["scala", "sbt"]
    },
    "clojure-lsp": {
      "command": "clojure-lsp",
      "rootPatterns": ["project.clj"],
      "filetypes": ["clojure"],
      "additionalSchemes": ["jar", "zipfile"]
    }
  }
}
